    "react": "~15.2.0",
    "react-addons-pure-render-mixin": "~15.2.0",
    "react-dom": "~15.2.0",
    "image-size": "~0.5.0",
    "react-intl": "~2.1.3",
    "react-modal": "~1.4.0",
    "react-router": "~2.5.2",
    "react-addons-css-transition-group": "~15.2.0",
    "underscore": "~1.8.3",
    "react-autosize-textarea": "~0.3.1",
    "grunt-cli": "~1.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
